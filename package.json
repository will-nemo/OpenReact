{
  "name": "open-react",
  "version": "1.0.0",
  "description": "Open Source React Component Library",
  "author": "will-nemo",
  "license": "MIT",
  "homepage": "https://github.com/will-nemo/OpenReact#readme",
  "keywords": [
    "open",
    "open-react",
    "source",
    "react",
    "component",
    "library"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/will-nemo/OpenReact.git"
  },
  "bugs": {
    "url": "https://github.com/will-nemo/OpenReact/issues"
  },
  "scripts": {
    "build": "npm run clean && rollup -c && husky install",
    "build:local": "npm run build && npm pack",
    "clean": "rimraf dist",
    "test": "jest",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepare": "husky install",
    "prettier": "prettier --write src"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "*.{js,ts,tsx}": "eslint --cache --fix",
    "*.{js,ts,tsx,css,scss,md}": "prettier --write"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.15"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.22.9",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@rollup/plugin-commonjs": "^25.0.3",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-terser": "^0.4.3",
    "@storybook/addon-essentials": "^7.1.0",
    "@storybook/addon-interactions": "^7.1.0",
    "@storybook/addon-links": "^7.1.0",
    "@storybook/addon-onboarding": "^1.0.7",
    "@storybook/blocks": "^7.1.0",
    "@storybook/react": "^7.1.0",
    "@storybook/react-webpack5": "^7.1.0",
    "@storybook/testing-library": "^0.2.0",
    "@testing-library/react": "^14.0.0",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/rollup-plugin-peer-deps-external": "^2.2.1",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "jest": "^29.6.1",
    "lint-staged": "^13.2.3",
    "prettier": "3.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.0",
    "rollup": "^2.79.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-scss": "^4.0.0",
    "rollup-plugin-typescript2": "^0.35.0",
    "sass": "^1.63.6",
    "sass-loader": "^13.3.2",
    "storybook": "^7.1.0",
    "typescript": "^5.1.6"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16"
  },
  "files": [
    "dist"
  ],
  "browser": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts"
}
